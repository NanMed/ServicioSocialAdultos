<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="false">

<display-name>
	Adulto mayores
</display-name>

<description>
	Blog
</description>

<session-config>
	<session-timeout>
		30
	</session-timeout>
</session-config>

<context-param> 
	<description>
		User
	</description> 
	<param-name>
		usuario
	</param-name> 
	<param-value>
		adultos
	</param-value> 
</context-param>

<context-param> 
	<description>
		Password
	</description> 
	<param-name>
		pass
	</param-name> 
	<param-value>
		mipass
	</param-value> 
</context-param>

<context-param> 
	<description>
		Database
	</description> 
	<param-name>
		base
	</param-name> 
	<param-value>
		adultos
	</param-value> 
</context-param> 

<jsp-config>
	<jsp-property-group>
		<url-pattern>
			*.jsp
		</url-pattern>
		<page-encoding>
			UTF-8
		</page-encoding>
	</jsp-property-group>
</jsp-config>


</web-app>
